{
  "description": " Multiple tabs ain't stressfull anymore. Clean and simple tab management. See https://github.com/himanshuc3/multipin",
  "manifest_version": 2,
  "name": "Multipin",
  "version": "1.0",
  "homepage_url": "https://github.com/himanshuc3/multipin",
  "icons": {
    "16": "images/logo.svg",
    "48": "images/logo.svg",
    "128": "images/logo.svg"
  },

  "permissions": [
    "tabs",
    "<all_urls>",
    "sessions"
  ],

  "browser_action": {
    "default_icon": "images/logo.svg",
    "default_title": "Multipin",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  }

}
